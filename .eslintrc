{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "plugins": ["import"],
  "rules": {
    "semi": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "max-len": [0],

    "import/extensions": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "node": true,
    "jest": true
  }
}
